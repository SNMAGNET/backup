 *
 */
public class Person {

private String name = null;
private int age = 0;
private String mail= null;
private String address =null;
private int tell =0;

  private Person(String name,
		         int age,
		         String mail,
		         String address,
		         int tell){
	  this.name=name;
	  this.age=age;
	  this.mail=mail;
	  this.address=address;
	  this.tell=tell;
  }

  public String getName(){
	  return  this.name;
  }
  public void setName(String name){
	  this name = name;
  }
  ;
  public String getName(){
	  return  this.mail;
  };
  public String getName(){
	  return  this.address;
  };
  public int getName(){
	  return  this.age;
  };
  public int getName(){
	  return  this.tell;
  };


}
