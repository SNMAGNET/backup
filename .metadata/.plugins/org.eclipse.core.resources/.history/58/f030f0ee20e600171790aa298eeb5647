 */
public class SuperPhone extends SmartPhone {

	public void fly(){
		System.out.println("そらをとぶ");
		}
	public void atack(){
		System.out.println("はたく");
	}
	public void escape(){
		System.out.println("にげる");
	}
	public void ball(){
		System.out.println("モ○スターボールを投げる");
	}
	public void summon(){
		System.out.println("Dr.ブームを２体召喚する。その雄たけびは発動する。");
	}

}
