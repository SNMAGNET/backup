/**
 * @author internousdev
 *
 */
public class Person {

private String name = null;
private int age = 0;
private String mail= null;
private String address =null;
private int tell =0;

  private Person(String name,
		         int age,
		         String mail,
		         String address,
		         int tell){
	  this.name=name;
	  this.age=age;
	  this.mail=mail;
	  this.address=address;
	  this.tell=tell;
  }

  public String getName(){
	  return  this.name;
  }
  public void setName(String name){
	  this.name = name;
  }
  public String getName2(){
	  return  this.mail;
  }
  public void setName2(String mail){
	  this.mail = mail;
  }


  public String getName3(){
	  return  this.address;
  }
  public void setName3(String address){
	  this.address = address;
  }
  public int getName4(){
	  return  this.age;
  }public void setName4(int age){
	  this.age = age;
  }
  public int getName5(){
	  return  this.tell;
  }public void setName5(int tell){
	  this.tell = tell;
  }


}
