public class Person {

	public String name = null;
	public int age = 0;

		public String getName(){
	    	  return this.name;
	      }

	      public void setName(String name){
	    	  this.name = name;
	      }
	     public int getName1(){
	    	  return this.age;
	      }

	     public void setName(int age){
	    	  this.age = age;
	      }

}
